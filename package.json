{
  "name": "coorpacademy-mobile",
  "version": "1.1.60",
  "private": true,
  "scripts": {
    "postinstall": "npm run update:version && npm run -s pull:firebase:config",
    "cocoapods:install": "cd ios && (bundle check || bundle install) && bundle exec pod install",
    "precommit": "npm run storybook:generate-stories",
    "update:version": "node scripts/update-version.js",
    "cleanup": "watchman watch-del .",
    "lint:flow": "flow",
    "lint": "eslint .",
    "flow": "npm run lint:flow",
    "lint:fix": "npm run lint -- --fix",
    "check:imagemagick": "which convert || brew install imagemagick",
    "generate:assets": "npm run -s check:imagemagick && babel-node scripts/generate-assets.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:clean": "npm run cleanup && npm start -- --reset-cache",
    "start:e2e": "REACT_NATIVE_FLAVOR=E2E npm run -s start:clean",
    "start:storybook": "REACT_NATIVE_FLAVOR=STORYBOOK npm run start:clean",
    "prestart:ios": "npm run -s xcode:check && npm run -s cocoapods:install",
    "start:ios": "react-native run-ios",
    "start:ios:release": "npm run -s start:ios -- --configuration Release",
    "start:android": "react-native run-android",
    "start:android:release": "npm run start:android -- --variant=release",
    "prebuild:ios:cmd": "npm run -s xcode:check",
    "build:ios:cmd": "cd ios && export RCT_NO_LAUNCH_PACKAGER=1 && xcodebuild -workspace Coorpacademy.xcworkspace -scheme Coorpacademy -derivedDataPath build",
    "build:iphonesimulator:debug": "REACT_NATIVE_FLAVOR=E2E npm run build:ios:cmd -- -configuration Debug -sdk iphonesimulator | xcpretty",
    "build:iphonesimulator:release": "REACT_NATIVE_FLAVOR=E2E npm run build:ios:cmd -- -configuration Release -sdk iphonesimulator | xcpretty",
    "build:androidemulator:debug": "cd android && export REACT_NATIVE_FLAVOR=E2E && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
    "build:androidemulator:release": "cd android && export REACT_NATIVE_FLAVOR=E2E && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
    "test:end2end:ios:debug": "npm run test:end2end:ios:debug:build && npm run test:end2end:ios:debug:test",
    "test:end2end:ios:debug:build": "detox build --configuration=ios.simulator.debug",
    "test:end2end:ios:debug:test": "detox test --configuration=ios.simulator.debug",
    "test:end2end:ios:release": "npm run test:end2end:ios:release:build && npm run test:end2end:ios:release:test",
    "test:end2end:ios:release:build": "detox build --configuration=ios.simulator.release",
    "test:end2end:ios:release:test": "detox test --configuration=ios.simulator.release",
    "test:end2end:android:debug": "npm run test:end2end:android:debug:build && npm run test:end2end:android:debug:test",
    "test:end2end:android:debug:build": "detox build --configuration=android.emulator.debug",
    "test:end2end:android:debug:test": "detox test --configuration=android.emulator.debug",
    "test:end2end:android:release": "npm run test:end2end:android:release:build && npm run test:end2end:android:release:test",
    "test:end2end:android:release:build": "detox build --configuration=android.emulator.release",
    "test:end2end:android:release:test": "detox test --configuration=android.emulator.release",
    "test:unit": "jest src -c jest.config.unit.js",
    "test": "npm run lint && npm run flow && npm run test:unit && npm run test:end2end:ios:release",
    "storybook:generate-stories": "babel-node scripts/generate-stories.js",
    "pull:certificate:ios": "cd ios && fastlane match development --readonly && fastlane match adhoc --readonly",
    "pull:certificate:android": "node scripts/pull-certificate-android.js",
    "pull:firebase:config": "babel-node scripts/pull-firebase-config.js",
    "generate:bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "generate:bundle:ios": "react-native bundle --entry-file index.js --platform ios --bundle-output ios/main.jsbundle",
    "pull:translations": "babel-node scripts/pull-translations.js",
    "postpull:translations": "eslint --fix src/translations",
    "notify:publication": "node scripts/notify-publication.js",
    "postversion": "react-native-version --never-increment-build",
    "version:should-release": "node scripts/should-release.js \"$(git log -1 --pretty=%B)\"",
    "version:release-type": "node scripts/get-version-increment-for-commit.js \"$(git log -1 --pretty=%B)\"",
    "version:auto-bump": "npm -s run version:should-release && npm version $(npm -s run version:release-type) -m 'v%s' || echo 'No bump is needed'",
    "version:increment": "npm version -m '%s'",
    "xcode:check": "xcoderc"
  },
  "engines": {
    "yarn": "^1.12.3"
  },
  "dependencies": {
    "@coorpacademy/acl": "^1.0.1",
    "@coorpacademy/nova-icons": "3.1.5",
    "@coorpacademy/player-services": "5.0.5",
    "@coorpacademy/player-store": "4.0.3",
    "@coorpacademy/progression-engine": "^11.0.1",
    "@coorpacademy/react-native-deck-swiper": "^1.5.35",
    "@coorpacademy/react-native-slider": "^0.11.1",
    "@coorpacademy/react-native-video-controls": "2.5.5",
    "app-store-link": "^1.0.2",
    "apsl-react-native-button": "^3.1.1",
    "bson": "^4.0.2",
    "color": "^3.1.0",
    "cross-fetch": "^3.0.1",
    "hoist-non-react-statics": "^3.2.1",
    "jwt-decode": "^2.2.0",
    "p-map": "^2.1.0",
    "react": "16.4.1",
    "react-native": "^0.56.0",
    "react-native-camera": "^1.12.0",
    "react-native-card-flip": "^1.0.4",
    "react-native-confetti-cannon": "^1.1.1",
    "react-native-device-info": "^1.1.0",
    "react-native-gesture-handler": "^1.0.10",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-localization": "^2.0.3",
    "react-native-modal": "^11.0.1",
    "react-native-orientation-locker": "1.1.5",
    "react-native-pdf": "5.0.9",
    "react-native-picker-select": "^6.0.0",
    "react-native-portal": "^1.3.0",
    "react-native-qrcode-scanner": "^1.1.2",
    "react-native-render-html": "3",
    "react-native-sideswipe": "1.4.2",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.2.0",
    "react-native-svg": "^6.0.0",
    "react-native-video": "3.2.1",
    "react-navigation": "^3.0.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "0.10.13",
    "semver": "^5.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@coorpacademy/eslint-plugin-coorpacademy": "^8.3.1",
    "@slack/client": "^4.12.0",
    "@storybook/addon-actions": "^4.0.6",
    "@storybook/addon-links": "^4.0.6",
    "@storybook/addon-storyshots": "^4.0.6",
    "@storybook/addons": "^4.0.6",
    "@storybook/cli": "^4.0.6",
    "@storybook/react-native": "^4.0.6",
    "aws-sdk": "^2.355.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "detox": "^12.1.2",
    "eslint": "^5.5.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "^0.75.0",
    "globby": "^8.0.1",
    "husky": "^0.14.3",
    "imagemagick": "^0.1.3",
    "jest": "^23.5.0",
    "jest-junit": "^6.3.0",
    "metro-react-native-babel-preset": "^0.49.0",
    "mkdirp": "^0.5.1",
    "opn-cli": "^4.0.0",
    "react-dom": "16.4.1",
    "react-native-firebase": "^5.2.3",
    "react-native-version": "^2.7.0",
    "react-test-renderer": "16.4.1",
    "regenerator-runtime": "^0.12.1",
    "shelljs": "^0.8.3",
    "xcoderc": "^1.0.5"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "jest.config.e2e.js",
    "configurations": {
      "ios.simulator.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Coorpacademy.app",
        "build": "npm run build:iphonesimulator:debug",
        "type": "ios.simulator",
        "name": "iPhone 6"
      },
      "ios.simulator.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/Coorpacademy.app",
        "build": "npm run build:iphonesimulator:release",
        "type": "ios.simulator",
        "name": "iPhone 6"
      },
      "android.emulator.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/Coorpacademy-debug.apk",
        "build": "npm run build:androidemulator:debug",
        "type": "android.emulator",
        "name": "Nexus_5X_API_19"
      },
      "android.emulator.release": {
        "binaryPath": "android/app/build/outputs/apk/release/Coorpacademy-release.apk",
        "build": "npm run build:androidemulator:release",
        "type": "android.emulator",
        "name": "Nexus_5X_API_19"
      }
    }
  }
}
